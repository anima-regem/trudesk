    "build": "npm run clean && grunt build --force",
    "test": "ts-mocha --recursive",
    "lint": "eslint \"src/**\"",
    "clean": "rimraf dist/*",
    "format": "npx prettier \"./*.js\" \"src/*.js\" \"src/**/*.js\" \"src/**/*.jsx\" \"!src/public/js/vendor/**/*.js\" \"!src/public/js/plugins/**/*.js\"",
    "coverage": "nyc --reporter=lcovonly npm run test",
    "codacy": "npm run coverage && bash <(curl -Ls https://coverage.codacy.com/get.sh) report -r ./coverage/lcov.info && rm -rf ./coverage",
    "webpackwatch": "webpack --config webpack.config.js --watch --progress",
    "webpackdev": "webpack --config webpack.config.js --progress",
    "webpackdist": "rimraf dist/public && webpack --env production --node-env production --config webpack.config.js",
    "snyk-protect": "snyk protect"
  },
